<form method="get" action="/quizes">
  <p>
    <label for="search">Buscar pregunta: </label>
  </p>
  <input type="text" name="search" />
  <input type="submit" value="Buscar" />
</form>

<ul>
  <% for(var i=0; i < quizes.length; i++) { %>
    <li>
      <a href="/quizes/<%= quizes[i].id %>">
        <%= quizes[i].pregunta %>
      </a>
    </li>
  <% } %>
</ul>
